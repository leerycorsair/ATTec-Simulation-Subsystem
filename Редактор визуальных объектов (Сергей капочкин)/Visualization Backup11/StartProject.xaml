        <TextBox Height="23" HorizontalAlignment="Left" Margin="91,97,0,0" Name="textBox2" VerticalAlignment="Top" Width="425" Grid.Column="1" />
        <Button Content="Установить" Height="23" HorizontalAlignment="Left" Margin="42,196,0,0" Name="button1" VerticalAlignment="Top" Width="114" Grid.Column="1" />
        <Button Content="Выйти" Height="23" HorizontalAlignment="Left" Margin="197,196,0,0" Name="button2" VerticalAlignment="Top" Width="114" Grid.Column="1" />
        <Button Content="Помощь" Height="23" HorizontalAlignment="Right" Margin="0,196,97,0" Name="button3" VerticalAlignment="Top" Width="114" Grid.Column="1" />
