<Модель>
	<Тип_ресурсов Имя_типа_ресурсов="Парикмахерские" Вид_типа_ресурсов="Постоянные">
		<Параметр_типа Имя_параметра="Вход" Тип_параметра="enum {нет_клиентов, клиент_прибыл}" Умолчание=""/>
		<Параметр_типа Имя_параметра="Количество_клиентов_в_очереди" Тип_параметра="int" Умолчание=""/>
		<Параметр_типа Имя_параметра="Занятость_парикмахера" Тип_параметра="enum {Свободен, Занят}" Умолчание="Свободен"/>
		<Параметр_типа Имя_параметра="Обслужено_клиентов" Тип_параметра="int" Умолчание=""/>
	</Тип_ресурсов>
	<Тип_ресурсов Имя_типа_ресурсов="Клиенты" Вид_типа_ресурсов="Постоянные">
		<Параметр_типа Имя_параметра="Номер" Тип_параметра="int" Умолчание=""/>
		<Параметр_типа Имя_параметра="Статус" Тип_параметра="enum {нет, в_очереди, в_стрижке}" Умолчание="нет"/>
		<Параметр_типа Имя_параметра="Время_прихода" Тип_параметра="double" Умолчание="0.0"/>
		<Параметр_типа Имя_параметра="Позиция_в_очереди" Тип_параметра="int" Умолчание="0"/>
	</Тип_ресурсов>
	<Ресурс Имя_ресурса="Парикмахерская" Имя_типа_ресурсов="Парикмахерские" Трассировка="Да" Начальные_значения="{нет_клиентов, 3, *, 0}"/>
	<Ресурс Имя_ресурса="Клиент_1" Имя_типа_ресурсов="Клиенты" Трассировка="Нет" Начальные_значения="{1, в_очереди, 0.0, 1}"/>
	<Ресурс Имя_ресурса="Клиент_2" Имя_типа_ресурсов="Клиенты" Трассировка="Нет" Начальные_значения="{2, в_очереди, 0.0, 3}"/>
	<Ресурс Имя_ресурса="Клиент_3" Имя_типа_ресурсов="Клиенты" Трассировка="Нет" Начальные_значения="{3, в_очереди, 0.0, 4}"/>
	<Ресурс Имя_ресурса="Клиент_4" Имя_типа_ресурсов="Клиенты" Трассировка="Нет" Начальные_значения="{4, *, *, *}"/>
	<Ресурс Имя_ресурса="Клиент_5" Имя_типа_ресурсов="Клиенты" Трассировка="Нет" Начальные_значения="{5, *, *, *}"/>
	<Ресурс Имя_ресурса="Клиент_6" Имя_типа_ресурсов="Клиенты" Трассировка="Нет" Начальные_значения="{6, *, *, *}"/>
	<Образец_операции Имя_образца="Образец_Приход_клиента" Тип_образца="Нерегулярное событие" Трассировка="Да">
		<Релевантный_ресурс Имя_релевантного_ресурса="Фигаро" Описатель="Парикмахерские" Статус_конвертора="Сохранить" Статус_конвертора_начала="" Статус_конвертора_конца=""/>
		<Время Тип="Случайное" Закон="@normal@" Значение="" Начало_интервала = "0.1" Конец_интервала = "0.9"/>
		<Тело_образца>
			<Релевантный_ресурс Имя_релевантного_ресурса="Фигаро">
				<Правило_использования Предусловие="" Convert_event="Вход = клиент_прибыл" Convert_rule="" Convert_begin="" Convert_end=""/>
			</Релевантный_ресурс>
		</Тело_образца>
	</Образец_операции>
	<Образец_операции Имя_образца="Образец_Размещение" Тип_образца="Правило" Трассировка="Да">
		<Релевантный_ресурс Имя_релевантного_ресурса="Клиент" Описатель="Клиенты" Статус_конвертора="Сохранить" Статус_конвертора_начала="" Статус_конвертора_конца=""/>
		<Релевантный_ресурс Имя_релевантного_ресурса="Фигаро" Описатель="Парикмахерские" Статус_конвертора="Сохранить" Статус_конвертора_начала="" Статус_конвертора_конца=""/>
		<Тело_образца>
			<Релевантный_ресурс Имя_релевантного_ресурса="Клиент">
				<Правило_использования Предусловие="Клиент.Статус = нет" Convert_event="" Convert_rule="Статус = в_очереди; Время_прихода = timeNow; Позиция_в_очереди = Выбор_места_в_очереди(0)" Convert_begin="" Convert_end=""/>
			</Релевантный_ресурс>
			<Релевантный_ресурс Имя_релевантного_ресурса="Фигаро">
				<Правило_использования Предусловие="Фигаро.Вход = клиент_прибыл @and@ Фигаро.Количество_клиентов_в_очереди @less@ 6" Convert_event="" Convert_rule="Вход =нет_клиентов; Количество_клиентов_в_очереди = Фигаро.Количество_клиентов_в_очереди + 1" Convert_begin="" Convert_end=""/>
			</Релевантный_ресурс>
		</Тело_образца>
		<Время Тип="Точное" Закон="" Значение="1" Начало_интервала = "" Конец_интервала = ""/>
	</Образец_операции>
	<Образец_операции Имя_образца="Образец_Обслуживание_клиента" Тип_образца="Операция" Трассировка="Да">
		<Релевантный_ресурс Имя_релевантного_ресурса="Фигаро" Описатель="Парикмахерские" Статус_конвертора="" Статус_конвертора_начала="Сохранить" Статус_конвертора_конца="Сохранить"/>
		<Релевантный_ресурс Имя_релевантного_ресурса="Клиент" Описатель="Клиенты" Статус_конвертора="" Статус_конвертора_начала="Сохранить" Статус_конвертора_конца="Сохранить"/>
		<Время Тип="Точное" Закон="" Значение="1" Начало_интервала = "" Конец_интервала = ""/>
		<Тело_образца>
			<Релевантный_ресурс Имя_релевантного_ресурса="Фигаро">
				<Правило_использования Предусловие="Фигаро.Количество_клиентов_в_очереди > 0 @and@ Фигаро.Занятость_парикмахера = Свободен" Convert_event="" Convert_rule="" Convert_begin="Занятость_парикмахера = Занят" Convert_end="Количество_клиентов_в_очереди = Фигаро.Количество_клиентов_в_очереди - 1; Занятость_парикмахера = Свободен; Обслужено_клиентов = Фигаро.Обслужено_клиентов + 1"/>
			</Релевантный_ресурс>
			<Релевантный_ресурс Имя_релевантного_ресурса="Клиент">
				<Правило_использования Предусловие="Клиент.Статус = в_очереди" Convert_event="" Convert_rule="" Convert_begin="Статус = в_стрижке; Позиция_в_очереди = 0" Convert_end="Статус = нет"/>
			</Релевантный_ресурс>
		</Тело_образца>
	</Образец_операции>
	<Операция Имя_операции="Приход_клиента" Имя_образца="Образец_Приход_клиента"/>
	<Операция Имя_операции="Размещение" Имя_образца="Образец_Размещение"/>
	<Операция Имя_операции="Обслуживание_клиента" Имя_образца="Образец_Обслуживание_клиента"/>
	<Функция Имя_функции="Выбор_места_в_очереди" Возвращаемый_тип="int" Тело_функции="if (Клиент_1.Позиция_в_очереди != 1 @and@ Клиент_2.Позиция_в_очереди != 1 @and@ Клиент_3.Позиция_в_очереди != 1 @and@ Клиент_4.Позиция_в_очереди != 1 @and@ Клиент_5.Позиция_в_очереди != 1 @and@ Клиент_6.Позиция_в_очереди != 1) {Выбор_места_в_очереди = 1;} if (Клиент_1.Позиция_в_очереди !=2 @and@ Клиент_2.Позиция_в_очереди !=2 @and@ Клиент_3.Позиция_в_очереди !=2 @and@ Клиент_4.Позиция_в_очереди !=2 @and@ Клиент_5.Позиция_в_очереди !=2 @and@ Клиент_6.Позиция_в_очереди != 2) {Выбор_места_в_очереди = 2;} if (Клиент_1.Позиция_в_очереди !=3 @and@ Клиент_2.Позиция_в_очереди !=3 @and@ Клиент_3.Позиция_в_очереди !=3 @and@ Клиент_4.Позиция_в_очереди !=3 @and@ Клиент_5.Позиция_в_очереди !=3 @and@ Клиент_6.Позиция_в_очереди != 3) {Выбор_места_в_очереди = 3;} if (Клиент_1.Позиция_в_очереди !=4 @and@ Клиент_2.Позиция_в_очереди !=4 @and@ Клиент_3.Позиция_в_очереди !=4 @and@ Клиент_4.Позиция_в_очереди !=4 @and@ Клиент_5.Позиция_в_очереди !=4 @and@ Клиент_6.Позиция_в_очереди != 4) {Выбор_места_в_очереди = 4;} if (Клиент_1.Позиция_в_очереди !=5 @and@ Клиент_2.Позиция_в_очереди !=5 @and@ Клиент_3.Позиция_в_очереди !=5 @and@ Клиент_4.Позиция_в_очереди !=5 @and@ Клиент_5.Позиция_в_очереди !=5 @and@ Клиент_6.Позиция_в_очереди != 5) {Выбор_места_в_очереди = 5;} if (Клиент_1.Позиция_в_очереди !=6 @and@ Клиент_2.Позиция_в_очереди !=6 @and@ Клиент_3.Позиция_в_очереди !=6 @and@ Клиент_4.Позиция_в_очереди !=6 @and@ Клиент_5.Позиция_в_очереди !=6 @and@ Клиент_6.Позиция_в_очереди != 6) {Выбор_места_в_очереди = 6;}">
		<Параметр_функции Имя_параметра_функции="Пусто" Тип_параметра_функции="int"/>
	</Функция>
<Функция Имя_функции="Распределение_норм" Возвращаемый_тип="int" Тело_функции="@normal@">
	<Параметр_функции Имя_параметра_функции="" Тип_параметра_функции=""/>
	</Функция>
</Модель>